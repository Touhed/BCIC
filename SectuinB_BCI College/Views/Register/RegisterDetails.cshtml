@model GroupB01_BCIC.Models.Register
@{
    ViewData["Title"] = "Register";
}


<h1 style="text-align:center">Register Information</h1>
<div class="container">
    <table border="1" class="table table-bordered">
        <thead>
            <tr>
                <td style="text-align:center; font-weight: bold;">Register Id</td>
                <td style="text-align:center; font-weight: bold;">Register Name</td>
                <td style="text-align:center; font-weight: bold;">Email</td>
                <td style="text-align:center; font-weight: bold;">Contact</td>
                <td style="text-align:center; font-weight: bold;">Address</td>
            </tr>
        </thead>

        <tr>
            <td style="text-align:center">@Model.RegisterId</td>
            <td style="text-align:center">@Model.RegisterName</td>
            <td style="text-align:center">@Model.Email</td>
            <td style="text-align:center">@Model.Contact</td>
            <td style="text-align:center">@Model.Address</td>
        </tr>
    </table>
</div>
