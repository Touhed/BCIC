
@{
    Layout = "_LayoutForStudent";
    ViewData["Title"] = "English Version Student";
}

<!DOCTYPE html>
<html>
<body style="background-color: darkgray; text-align: center; font-family: 'Times New Roman', Times, serif;">
    <div class="form-group">
        <h2 style="text-align: center;">Choose For Create Account</h2>
        <hr />
        <form asp-controller="EnglishVS" asp-action="AddEngArtStudent" method="get">
            <button class="btn btn-primary btn-lg">Add English Version Arts Student</button>
        </form>
        <br />
        <form asp-controller="EnglishVS" asp-action="AddEngComStudent" method="get">
            <button class="btn btn-primary btn-lg">Add English Version Commerce Student</button>
        </form>
        <br />
        <form asp-controller="EnglishVS" asp-action="AddEngSciStudent" method="get">
            <button class="btn btn-primary btn-lg">Add English Version Science Student</button>
        </form>
    </div>
</body>
</html>